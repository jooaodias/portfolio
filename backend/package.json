{
  "name": "portfolio-blog-api",
  "version": "1.0.0",
  "type": "module",
  "description": "API REST para o blog do portfólio com Fastify",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "lint": "eslint src --ext .ts"
  },
  "keywords": ["fastify", "blog", "api", "typescript"],
  "author": "João Aleixo",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@prisma/client": "^6.4.1",
    "fastify": "^5.2.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "prisma": "^6.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
